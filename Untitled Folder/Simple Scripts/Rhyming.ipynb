    "#!/usr/bin/python3\n",
    "\n",
    "import requests\n",
    "\n",
    "word = input(\"Pick a word: \")\n",
    "print()\n",
    "\n",
    "URL = \"https://api.datamuse.com/words\"\n",
    "\n",
    "parameter = {\"rel_rhy\":word}\n",
    "request = requests.get(URL,parameter)\n",
    "\n",
    "rhyme_json = request.json()\n",
    "for i in rhyme_json[0:3]:\n",
    " print(i['word'])"
